{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug cpp101 Project",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/Clang 16.0.0 x86_64-apple-darwin24.0.0/bin/cpp101",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "CMake: build",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/lldb"
        }
    ]
}